pipeline
{

   agent {
  label 'Ansible'
}

tools {
  maven 'maven'
}
    stages{
        stage('build')
        {
            steps {
                sh "this is build stage"
            }

            post {        //this will execute after the build is successful 
             success {
                 archiveArtifacts artifacts: '**/target/*.war'

    // One or more steps need to be included within each condition's block.
  }
}
        }


